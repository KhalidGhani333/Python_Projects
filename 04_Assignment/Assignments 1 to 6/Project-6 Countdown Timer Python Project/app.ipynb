   "source": [
    "import time\n",
    "\n",
    "def countdown(seconds):\n",
    "    while seconds > 0:\n",
    "        print(f'Time left {seconds} Seconds!')\n",
    "        time.sleep(1)\n",
    "        seconds -= 1\n",
    "\n",
    "    print(\"⏰ Time's up!\")\n",
    "\n",
    "seconds = int(input(\"Enter Time in Seconds: \"))\n",
    "\n",
    "countdown(seconds)"
   ]
  }
